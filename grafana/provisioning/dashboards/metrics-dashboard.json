{
  "id": null,
  "title": "Spring Boot Metrics Demo",
  "tags": ["spring", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "JVM Memory Used",
      "targets": [
        {
          "expr": "jvm_memory_used_bytes",
          "legendFormat": "{{area}}"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Custom Random Metric",
      "targets": [
        {
          "expr": "custom_random_metric",
          "legendFormat": "random value"
        }
      ],
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 }
    }
  ]
}
